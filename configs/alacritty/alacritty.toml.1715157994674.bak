live_config_reload = true
[env]
TERM = "xterm-256color"
[window]
decorations = "buttonless"
dynamic_padding = false
opacity = 0.6

[window.padding]
# x = 25
# y = 20
[font]
size = 10.0

[font.bold]
family = "Roboto Mono Nerd Font"
style = "Heavy"

[font.bold_italic]
family = "Roboto Mono Nerd Font"
style = "Heavy Italic"

[font.italic]
family = "Roboto Mono Nerd Font"
style = "Medium Italic"

[font.normal]
family = "Roboto Mono Nerd Font"
style = "Medium"

