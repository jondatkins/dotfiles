live_config_reload = true
import = [
  "/usr/lib/node_modules/alacritty-themes/themes/3024.dark.toml"
]

[env]
TERM = "xterm-256color"

[window]
decorations = "buttonless"
dynamic_padding = false
opacity = 0.6
padding = { }

[font]
size = 10

  [font.bold]
  family = "Roboto Mono Nerd Font"
  style = "Heavy"

  [font.bold_italic]
  family = "Roboto Mono Nerd Font"
  style = "Heavy Italic"

  [font.italic]
  family = "Roboto Mono Nerd Font"
  style = "Medium Italic"

  [font.normal]
  family = "Roboto Mono Nerd Font"
  style = "Medium"
